{
  "name": "account",
  "base": "User",
  "strict": true,
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "watchList": {
      "type": [
        "string"
      ],
      "default": []
    },
    "email": {
      "type": "string",
      "required": false
    },
    "notification_tokens": {
      "type": [
        "string"
      ],
      "default": []
    },
    "notification_token": {
      "type": "string",
      "required": false
    },
    "password": {
      "type": "string",
      "required": false
    },
    "created_at": {
      "type": "date",
      "defaultFn": "now"
    },
    "feedState": {
      "type": {
        "news": ["string"],
        "ad": ["string"],
        "user": ["string"],
        "slot": "number",
        "lastReset": {
          "type": "date",
          "defaultFn": "now"
        }
      }
    }
  },
  "validations": [],
  "relations": {
    "_wallets": {
      "type": "embedsMany",
      "model": "wallet",
      "property": "wallets",
      "default": [],
      "options": {
        "forceId": false
      }
    },
    "_addresses": {
      "type": "embedsMany",
      "model": "address",
      "property": "addresses",
      "default": [],
      "options": {
        "forceId": false
      }
    },
    "_feed": {
      "type": "embedsMany",
      "model": "feed",
      "property": "feed",
      "default": [],
      "options": {
        "forceId": true,
        "validate": true,
        "persistent": true
      }
    },
    "_exchangeAccounts": {
      "type": "embedsMany",
      "model": "ExchangeAccount",
      "property": "exchangeAccounts",
      "default": [],
      "options": {
        "forceId": false
      }
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "ownsEmbedded",
      "permission": "ALLOW",
      "property": "__create___exchangeAccounts"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY",
      "property": "getPortfolio"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "getPortfolio"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY",
      "property": "getPortfolioTicker"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "getPortfolioChart"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY",
      "property": "refreshAddress"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "refreshAddress"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY",
      "property": "getTokenMeta"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "getTokenMeta"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "addToWatchList"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "removeFromWatchList"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "addAddress"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "deleteAddress"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "addWallet"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "register"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "addNotificationToken"
    }
  ],
  "methods": {}
}
