{
  "name": "Alert",
  "base": "PersistedModel",
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "frequency": {
      "type": "number",
      "default": 0
    },
    "status": {
      "type": "boolean",
      "default": true
    },
    "createdAt": {
      "type": "date",
      "defaultFn": "now"
    },
    "updatedAt":{
      "type": "date"
    }
  },
  "validations": [],
  "relations": {
    "user": {
      "type": "belongsTo",
      "model": "account"
    },
    "alert": {
      "type": "belongsTo",
      "model": "KapacitorAlert"
    }
  },
  "acls": [],
  "methods": {}
}
